<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fa="http://schemas.fontawesome.io/icons/">

    <SolidColorBrush x:Key="DarkButtonBackgroundHighlight" Color="#2F7F6F" />
    <SolidColorBrush x:Key="DarkButtonBackground" Color="#555" />
    <SolidColorBrush x:Key="DarkBackground" Color="#3E3E3E" />
    <SolidColorBrush x:Key="LightBackground" Color="#C1C1C1" />
    <SolidColorBrush x:Key="DisabledBackground" Color="#AAA" />
    <SolidColorBrush x:Key="GreenHighlight" Color="#CCFF00" />
    <SolidColorBrush x:Key="BlueHighlight" Color="#31D6FF" />
    <SolidColorBrush x:Key="PinkHighlight" Color="#FF3FAC" />
    <SolidColorBrush x:Key="Foreground" Color="#EEE" />
    <SolidColorBrush x:Key="MediumDarkForeground" Color="#7E7E7E" />
    <SolidColorBrush x:Key="LightForeground" Color="#FFF" />
    <SolidColorBrush x:Key="DisabledForeground" Color="#DDD" />
    <SolidColorBrush x:Key="DarkBorder" Color="#222" />
    <SolidColorBrush x:Key="LightBorder" Color="#B6B6B6" />
    <Color x:Key="SelectedHighlightColor" R="139" G="174" B="0" A="255"/>
    <SolidColorBrush x:Key="SelectedHighlight" Color="{DynamicResource SelectedHighlightColor}" />

</ResourceDictionary>